<body>
    <!-- 微云 -->
    <div class="weiyun">
        <!-- 头部 -->
        <% include html/header.html %>
        <!-- 主要内容区 -->
        <div class="weiyun-content">
            <!-- 左侧菜单区 -->
            <div class="lay-aside">

                <div id="_search_bar" class="mod-search">
                    <div class="search-panel">
                        <div class="search-input">
                            <input id="search" placeholder="搜索全部" autocomplete="off" type="text"
                                aria-label="输入文件名并按下回车进行搜索" tabindex="0" class="mod-input">
                        </div>
                        <div class="search-placeholder">
                            <i class="icon icon-searchs"></i>
                        </div>
                    </div>

                </div>
                <div class="aside-box">
                    <div class="aside-wrap">
                        <ul class="nav-box">

                            <li class="nav-list summary">
                                <a href="/summary" class="link doc" title="首页">
                                    <i></i>首页</a>
                            </li>
                            <li class="nav-list file ">
                                <a href="/file" class="link all" title="目录">
                                    <i></i>目录</a>
                            </li>

                            <li class="nav-list hightSearch">
                                <a href="/hightSearch" class="link pic" title="检索">
                                    <i></i>检索</a>
                            </li>
                            <li class="nav-list report">
                                <a href="/report" class="link" title="报表">
                                    <i class="icon-bar-chart"></i>报表</a>
                            </li>

                            <li class="nav-list access">
                                <a href="/access" class="link" title="查阅调阅">
                                    <i class="icon-search"></i>查阅调阅</a>
                            </li>

                            <li class="nav-list designSet nav-current">
                                <a href="/designSet" class="link" title="门类功能">
                                    <i class="icon-cogs"></i>门类功能</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- 右侧内容区域 -->
            <div class="main">
                <!-- 工具菜单导航 -->
                <div class="nav">
                    <nav class="nav-collects clearfix">

                        <div class="nav-prop" style="position: relative;">
                            <div style="float: left;margin: 6px 6px 0 10px;">
                                <button class="btn btn-primary" id="addProp">新增属性</button>
                            </div>
                        </div>
                        <div class="nav-table" style="position: relative;">
                            <div style="float: left;margin: 6px 6px 0 10px;">
                                <button class="btn btn-primary" id="addTable">新增分类</button>
                            </div>
                        </div>
                    </nav>
                </div>
                <!-- 主要内容展示区 -->
                <div class="content clearfix">
                    <!-- 树形导航 -->

                    <ul id="tree-menu" class="ztree span3">
                        <p style="text-align: center;margin-top: 20px">
                            <img src="/img/waiting.gif" />
                        </p>

                    </ul>
                    <!-- 文件展示区 -->
                    <div class="file-show">

                        <!-- 空白文件展示区 -->
                        <div class="g-empty sort-folder-empty">
                            <div class="empty-box">
                                <div class="ico"></div>
                                <p class="title">暂无文件</p>
                                <p class="content">请点击左上角的“上传”按钮添加文件</p>
                            </div>
                        </div>
                        <!-- 文件展示区 -->
                        <div class="file-list f_detail clearfix">
                            <table class="table table-hover table-striped table-bordered" width="100%" cellpadding="0"
                                cellspacing="0" border="0" id="table_list">
                                <thead>
                                    <tr class="tops">
                                        <th width="20px" style="text-align: center !important;"><input id="checkAll"
                                                type="checkbox"></th>
                                        <th style="">字段名称</th>
                                        <th style="">字段长度</th>
                                        <th style="">操作</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- 提示信息 -->
    <div class="full-tip-box">
        <span class="full-tip">
            <span class="inner">
                <i class="ico"></i>
                <span class="text" data-id="label"></span>
            </span>
        </span>
    </div>
    <div id="propType" style="display: none">
        <ul>
            <li type="1">删除分类属性</li>
        </ul>
    </div>
    <div class="pdialogDiv hide">
        <% include dialog/frmDocPro.html %>
        <!-- <% include dialog/frmDocAssign.html %>
        <% include dialog/frmDocAudit.html %>
        <% include dialog/frmPassword.html %>
        <% include dialog/frmcombind.html %>
        <% include dialog/frmTable.html %> -->
    </div>


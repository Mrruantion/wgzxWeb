    <style>
        .file-show {
            padding-left: 162px;
        }

        #tree-menu {
            float: left;
            width: 160px;
            height: 100%;
            /* padding: 0 12px; */
            border-right: 1px solid #e6e9f0;
            overflow: auto;
            padding-top: 5px;
            background: #fff;
            padding: 0
        }

        .frmDocumentAssign {
            overflow-x: auto;
            overflow-y: auto;
            width: 280px;
            height: 150px;
            border: solid 1px #ccc;
            margin-left: 50px;
        }

        #divDocumentAssign:focus,
        #divDocAudit:focus,
        #divDocLock:focus,
        #divDocSort:focus,
        #divTableUser:focus {
            outline: none
        }

        #passDoc,
        #printCode,
        #lockDoc,
        #combind,
        #export,
        #import,
        #sort,
        #exportProp,
        #specialExport,
        #tableShow,
        #addProp,
        #addPropList,
        #addTable {
            font-size: 12px;
            padding: 6px;
            background-color: #0070c0;
            border-color: #0070c0;
            /* height: 26px; */
        }

        #qucodePrint,
        .qrcodePrint {
            width: 100%;
            padding: 10px;
            box-sizing: border-box;
        }

        .qrcodePrint li {
            float: left;
            width: 20%;
            margin-bottom: 10px;
        }

        .qrcodePrint li img {
            display: block;
        }

        .qrcodePrint span {
            word-wrap: break-word;
        }

        #frmPassword .form-control,
        #frmCombind .form-control {
            width: 60%;
            display: inline-block;
            box-sizing: border-box;
        }

        .icon-unlock {
            margin-left: 5px;
            color: green;
        }

        .icon-lock {
            margin-left: 5px;
            color: red;
        }

        .pagination ul {
            display: inline-block;
            *display: inline;
            margin-bottom: 0;
            margin-left: 0;
            -webkit-border-radius: 4px;
            -moz-border-radius: 4px;
            border-radius: 4px;
            *zoom: 1;
            -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
            -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
            box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
        }

        .pagination ul>li {
            display: inline;
        }

        .pagination ul>li:first-child>a,
        .pagination ul>li:first-child>span {
            border-left-width: 1px;
            -webkit-border-bottom-left-radius: 4px;
            border-bottom-left-radius: 4px;
            -webkit-border-top-left-radius: 4px;
            border-top-left-radius: 4px;
            -moz-border-radius-bottomleft: 4px;
            -moz-border-radius-topleft: 4px;
        }

        .pagination ul>.active>a,
        .pagination ul>.active>span {
            color: #999;
            cursor: default;
        }

        .pagination ul>li:last-child>a,
        .pagination ul>li:last-child>span {
            -webkit-border-top-right-radius: 4px;
            border-top-right-radius: 4px;
            -webkit-border-bottom-right-radius: 4px;
            border-bottom-right-radius: 4px;
            -moz-border-radius-topright: 4px;
            -moz-border-radius-bottomright: 4px;
        }

        .pagination ul>.disabled>span,
        .pagination ul>.disabled>a,
        .pagination ul>.disabled>a:hover,
        .pagination ul>.disabled>a:focus {
            color: #999;
            cursor: default;
            background-color: transparent;
        }

        .pagination ul>li>a,
        .pagination ul>li>span {
            float: left;
            padding: 4px 12px;
            line-height: 20px;
            text-decoration: none;
            background-color: #fff;
            border: 1px solid #ddd;
            border-left-width: 0;
        }

        .file-list {
            padding: 10px 10px
        }

        .file-list .row {
            margin-right: 0px;
            margin-top: 10px
        }

        .file-list .table {
            margin-bottom: 0px
        }

        .file-list .table-bordered>thead>tr>td,
        .file-list .table-bordered>thead>tr>th {
            border-bottom-width: 0px;
        }

        .file-list .dataTables_scroll .dataTables_scrollHead {
            height: unset;
        }

        #frmTableUser span {
            display: inline-block;
            position: relative;
            margin-right: 5px;
            line-height: 20px;
        }

        .tkey,
        .tlkey {
            margin: 0
        }

        #frmTableUser span .tkey {
            position: relative;
            top: 2.7px;
        }

        #frmSpecial .row {
            margin-right: 0;
            /* display: none */
        }

        #tree-menu .tree_li {
            padding: 0 10px;
            line-height: 30px;
            cursor: pointer;
        }

        #tree-menu .tree_li.active {
            background: #0070c0;
            color: #fff
        }

        #tree-menu .tree_li:hover {
            background: #0070c0;
            color: #fff
        }

        #propType {
            background: #fff;
            border-radius: 4px;
            padding: 5px;
            border: 1px solid #ccc;
        }

        #propType ul li {
            cursor: pointer
        }



        /* #fileAll_list .table>thead>tr>th {
            border-bottom: 0px;
        }

        #fileAll_list .table-bordered>tbody>tr>td,
        #fileAll_list .table-bordered>tbody>tr>th,
        #fileAll_list .table-bordered>tfoot>tr>td,
        #fileAll_list .table-bordered>tfoot>tr>th,
        #fileAll_list .table-bordered>thead>tr>td,
        #fileAll_list .table-bordered>thead>tr>th {
            border: 0px solid #ddd;
        } */
    </style>
